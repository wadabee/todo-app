datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  // M1 Macは以下の設定が必要
  // binaryTargets = ["native", "linux-musl", "linux-arm64-openssl-1.1.x"]
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  // email     String   @unique
  name      String
}
