{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "db:gen": "prisma generate",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc",
    "start": "node build/src/server.js",
    "docker:up:test": "docker-compose -f docker-compose.test.yml up -d && dotenv -e .env.test -- npx prisma db push",
    "test": "jest --testPathIgnorePatterns \".*repository.*|.*controllers.*\"",
    "test:repo": "dotenv -e .env.test -- jest --testPathPattern \".*repository.*\" -i",
    "test:repo:db": "npm run docker:up:test && npm run test:repo && docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.0.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "^27.4.7",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "prisma": "^3.8.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "redoc-express": "^1.0.0",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.14.1"
  }
}
